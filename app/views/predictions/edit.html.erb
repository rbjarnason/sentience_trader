<h1>Editing prediction</h1>

<%= error_messages_for :prediction %>

<% form_for(@prediction) do |f| %>
  <p>
    <b>Neuron strategy</b><br />
    <%= f.text_field :neural_strategy_id %>
  </p>

  <p>
    <b>Prediction value</b><br />
    <%= f.text_field :prediction_value %>
  </p>

  <p>
    <b>Error value</b><br />
    <%= f.text_field :error_value %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @prediction %> |
<%= link_to 'Back', predictions_path %>
